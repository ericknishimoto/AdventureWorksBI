SET LANGUAGE 'Brazilian'
declare @startDate date;
declare @endDate date;

select @startDate =
(
SELECT Convert(CHAR(8),
	(SELECT DATEFROMPARTS(
		(SELECT YEAR(
			(SELECT CAST(
			(SELECT Min(MinDate) MinDate FROM
				(SELECT Min(OrderDate) MinDate FROM [AdventureWorks_STG].[dbo].[StgSalesOrderHeader]
					UNION
					SELECT Min(DueDate) MinDate FROM [AdventureWorks_STG].[dbo].[StgSalesOrderHeader]
					UNION
					SELECT Min(ShipDate) MinDate FROM [AdventureWorks_STG].[dbo].[StgSalesOrderHeader]
					)
			as list)
		AS date) as MinDate)
		)AS Year
		),
	01, 01) as MinDate)
,112) as MinDate
);

select @endDate =
(
SELECT Convert(CHAR(8),
	(SELECT DATEFROMPARTS(
		(SELECT YEAR(
			(SELECT CAST(
			(SELECT MAX(MaxDate) MaxDate FROM
				(SELECT MAX(OrderDate) MaxDate FROM [AdventureWorks_STG].[dbo].[StgSalesOrderHeader]
					UNION
					SELECT MAX(DueDate) MaxDate FROM [AdventureWorks_STG].[dbo].[StgSalesOrderHeader]
					UNION
					SELECT MAX(ShipDate) MaxDate FROM [AdventureWorks_STG].[dbo].[StgSalesOrderHeader]
					)
			as list)
		AS date) as MaxDate)
		)AS Year
		),
	12, 31) as MaxDate)
,112) as MaxDate
);

with dateRange as
(
  select dt = @startDate
  where @startDate &lt; @endDate
  union all
  select dateadd(dd, 1, dt)
  from dateRange
  where dateadd(dd, 1, dt) &lt;= @endDate
)
	select
		convert(int, format(dt, 'yyyyMMdd')) as SKData,
		dt AS Data,
